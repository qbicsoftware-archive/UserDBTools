<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<portlet-app xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.0" xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd">
    <portlet>
    
        <description>Portlet for creating QBiC Projects</description>
        <portlet-name>User DB Input Application Portlet</portlet-name>
        <display-name>User DB Input</display-name>
        
        <portlet-class>qdbtools.main.CustomVaadinPortlet</portlet-class>
        <init-param>
            <name>UI</name>
            <value>qdbtools.main.QuserdbtoolsUI</value>
        </init-param>
        
        <init-param>
            <name>widgetset</name>
            <value>qdbtools.main.widgetset.QuserdbtoolsWidgetset</value>
        </init-param>
        
        
        <!-- Supported portlet modes and content types. -->
        <supports>
            <mime-type>text/html</mime-type>
            <portlet-mode>view</portlet-mode>
        </supports>
        
        <!-- Not always required but Liferay uses these. -->
        <portlet-info>
            <title>User DB Input</title>
            <short-title>User DB Input</short-title>
        </portlet-info>
    </portlet>
</portlet-app>
